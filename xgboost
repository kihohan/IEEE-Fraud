import xgboost as xgb
# arary로 바꺼줘야됨
Train_X_array = np.array(Train_X)
Train_Y_array = np.array(Train_Y)
Test_X_array = np.array(Test_X)
Test_Y_array = np.array(Test_Y)
# 파라미터
params = { 'max_depth':3,
           'eta': 0.1,
           'objective':'binary:logistic',
           'eval_metric':'logloss',
           'early_stoppings':100
        }
num_rounds = 400
# 이렇게 바꿔줘야된다함
dtrain = xgb.DMatrix(data = Train_X_array, label = Train_Y_array)
dtest = xgb.DMatrix(data = Test_X_array, label = Test_Y_array)
# 이건 뭔지 모르겟음
wlist = [(dtrain,'train'),(dtest,'eval') ]
# 모델 만들기
model = xgb.train(params = params , dtrain=dtrain , num_boost_round=num_rounds , evals=wlist )
predicted = xgb_model.predict(dtest)
from sklearn.metrics import * 
print (roc_auc_score(Test_Y_array, predicted))
